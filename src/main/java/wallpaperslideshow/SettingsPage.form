<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="trykowo.wpp.wallpaperplugin.SettingsPage">
  <grid id="27dc6" binding="panel" layout-manager="GridLayoutManager" row-count="4" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="0" left="0" bottom="0" right="0"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="e39f8" class="com.intellij.openapi.ui.TextFieldWithBrowseButton" binding="fileTextBox">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="7" anchor="1" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <toolTipText value="The root directory of all the images."/>
        </properties>
      </component>
      <component id="fe504" class="javax.swing.JCheckBox" binding="randomCheckBox">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="9" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Random"/>
          <toolTipText value="Images in sequence or random."/>
        </properties>
      </component>
      <component id="42508" class="javax.swing.JButton" binding="reloadFolderButton" default-binding="true">
        <constraints>
          <grid row="1" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="1" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Reload Folder"/>
          <toolTipText value="Refresh the images stored to select from."/>
        </properties>
      </component>
      <component id="b11be" class="javax.swing.JSpinner" binding="intervalSpinner">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="1" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
      </component>
      <component id="3cba7" class="javax.swing.JCheckBox" binding="directoryWalkCheckBox" default-binding="true">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Directory Walk"/>
          <toolTipText value="Whether plugin should search directories within this directory."/>
        </properties>
      </component>
      <vspacer id="69a06">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
        </constraints>
      </vspacer>
    </children>
  </grid>
</form>
